<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Memory</servlet-name>
        <servlet-class>com.graph.Memory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestUtil</servlet-name>
        <servlet-class>com.graph.RequestUtil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VirtualServers</servlet-name>
        <servlet-class>com.graph.VirtualServers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Memory2</servlet-name>
        <servlet-class>com.graph.Memory2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestUtil2</servlet-name>
        <servlet-class>com.graph.RequestUtil2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestUtil3</servlet-name>
        <servlet-class>com.graph.RequestUtil3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Memory3</servlet-name>
        <servlet-class>com.graph.Memory3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Memory</servlet-name>
        <url-pattern>/Memory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestUtil</servlet-name>
        <url-pattern>/RequestUtil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VirtualServers</servlet-name>
        <url-pattern>/VirtualServers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Memory2</servlet-name>
        <url-pattern>/Memory2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestUtil2</servlet-name>
        <url-pattern>/RequestUtil2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestUtil3</servlet-name>
        <url-pattern>/RequestUtil3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Memory3</servlet-name>
        <url-pattern>/Memory3</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
